{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/socket.io",
      "destination": "/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.js"
    }
  ],
  "routes": [
    {
      "src": "/socket.io",
      "dest": "/index.js",
      "headers": {
        "Upgrade": "websocket",
        "Connection": "Upgrade"
      }
    }
  ]
}